{
	"name": "emp-sync-base",
	"displayName": "emp-sync-base",
	"description": "It can sync something for emp",
	"version": "0.1.0",
	"publisher": "Benny",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/emp*",
		"onCommand:emp-sync-base.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "emp-sync-base.syncCommand",
				"title": "EMP Remote Sync"
			},
			{
				"command": "emp-sync-base.initCommand",
				"title": "EMP Init"
			}
		],
		"configuration": {
			"title": "EMP-sync-base",
			"properties": {
				"empSyncBase.fileURL": {
					"type": "array",
					"default": [],
					"description": "Remote file list for EMP project,properties is a array for obejct:{'url':'xxxxx','name':'xxxxx'}"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"git-promise": "^1.0.0",
		"vsce": "^1.77.0"
	}
}
